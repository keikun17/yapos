var new_option = '<%= @vendor_item.code %>';

var el = $("select[data-vendor-items-from-remote=true]");

console.log(new_option);

$.each(el, function(index,select_tag){


  var option = $("<option></option").attr("value", new_option).text(new_option);

  console.log("Adding to a select tag");
  console.log(select_tag);
  console.log('this option');
  console.log(option)

  $(select_tag).append(option);
});

