# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
.bundle
uploads
!/log/.keep
!/tmp/.keep

# Ignore public/assets, run rake assets:precompile on prod instead
/public/assets

# Ignore Code Climate coverage folder
coverage

# Ignore .env file read by the `dotenv-rails` gem
.env
/config/master.key

tags

.vscode
.byebug_history

/public/packs
/public/packs-test
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity
